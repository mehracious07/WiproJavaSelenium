package Serilization_Deserilization;

import java.io.FileOutputStream;
import java.io.ObjectOutputStream;

public class Serialization {

	public static void main(String[] args) {
		Student s1=new Student("Sanjay",22);
try {
	FileOutputStream f=new FileOutputStream("C:\\Users\\my pc\\eclipse-workspaceWipro\\SDET_WIPRO\\src\\Serilization_Deserilization\\Se_Desrilization");
    ObjectOutputStream out =new  ObjectOutputStream(f);
    out.writeObject(s1);
    f.close();
    System.out.print("Object Serialized");
}catch(Exception e) {
	e.printStackTrace();
}
	}

}
