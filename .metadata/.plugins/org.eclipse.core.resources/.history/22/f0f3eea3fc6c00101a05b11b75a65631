package July30_Package;

import java.util.ArrayList;

public class Collections_ArrayList {

	public static void main(String[] args) {
		ArrayList<String>a1=new ArrayList<>();
		a1.add("Sanjay");
		a1.add("Rajat");
		a1.add("Sachin");
		a1.add("Nitin");
		a1.add("Mayank");
		
		System.out.println(a1);
		System.out.println(a1.get(2));
	}

}
