import java.util.Scanner;
public class ReadingFromTheKeyboard {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the user id:");
		int a ;
		if(sc.hasNextInt()) {
			a = sc.nextInt();	
		}
		
		System.out.println(" The user id is :"+a);


	}

}
